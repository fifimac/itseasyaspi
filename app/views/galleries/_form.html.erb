<%= simple_form_for(@gallery , :html => {:multipart => true} ) do |f| %>
  <%= f.error_notification %>
  <%= f.hidden_field :submitted_by %>
  <%= f.input :step %>
  <%= f.input :image %>
  <%= f.input :image_comment, label: "Image description:"  %>
  <%= f.button :submit, label: "Submit"  %>
<% end %>

